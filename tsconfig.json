{
  "compilerOptions": {
    "target": "ES5",
    "experimentalDecorators": true,
    "outDir": "dist",
    "declaration": true,
    "rootDir": "./src",
    "removeComments": true,
    "lib":[
      "es2015"
    ],
    "noImplicitAny": true,
    "moduleResolution": "node"
  },
  "transpileOnly": false,
  "include": ["src"],
  "exclude": ["node_modules"]
}